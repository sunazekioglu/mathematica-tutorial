  3.788009318205392*^9}, {3.802196496664577*^9, 3.802196500212782*^9}, {
  3.802536278323105*^9, 
  3.802536315587834*^9}},ExpressionUUID->"b14afe6e-7e69-4748-8534-\
412d98c18c78"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"solutionRules", " ", "=", " ", 
  RowBox[{"ToRules", "@", 
   RowBox[{"Reduce", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"2", "a"}], " ", "\[Equal]", " ", 
        RowBox[{"y", " ", "+", " ", "x"}]}], ",", " ", 
       RowBox[{
        RowBox[{"2", "c"}], "\[Equal]", " ", 
        RowBox[{"x", "-", "y"}]}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "y"}], "}"}]}], "]"}]}]}]], "Input",
