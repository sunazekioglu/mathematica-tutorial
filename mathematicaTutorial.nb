     RowBox[{"currentMin", " ", "=", " ", 
      RowBox[{"First", "@", "data"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
