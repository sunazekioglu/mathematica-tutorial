 RowBox[{"variables", " ", "=", " ", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"Power", "[", 
      RowBox[{"x", ",", "#"}], "]"}], "*", 
     RowBox[{"Power", "[", 
      RowBox[{"y", ",", 
       RowBox[{"4", "-", "#"}]}], "]"}]}], "&"}], "/@", 
   RowBox[{"Range", "[", 
    RowBox[{"0", ",", "4"}], "]"}]}]}]], "Input",
